<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="com.exalt.report.listeners.ReporterListener"/>
    </listeners>
    <test name="ConduitLoginPageTestChrome">
        <parameter name="browser" value="Chrome"></parameter>
        <classes>
            <class name="com.exalt.tests.DemoTest"></class>
            <class name="com.exalt.tests.ConduitLoginPageTest"></class>
        </classes>
    </test>
    <test name="ConduitLoginPageTestFirefox">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="com.exalt.tests.DemoTest"></class>
            <class name="com.exalt.tests.ConduitLoginPageTest"></class>
        </classes>
    </test>
</suite>